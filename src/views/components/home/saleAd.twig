<h1>sale ad slider</h1>


<div class="tg-tickerbox test" style="background-color:rgb(158, 99, 65); width: 100%; float: left; padding: 0px 0; position: relative; border-bottom: 1px solid #ddd;">
    <div class="col-md-10" style="padding:10px;">
        <marquee scrolldelay="100" loop="-1" style="padding-top: 5px;">
            {% for offer in component.addoffer %}
                <a href="https://salla.design/dev-adn671ta9breebis/offers" style="color: #fff; padding-top: 5px; margin-right: 20px; text-decoration:none;">{{offer.offer}}</a>
            {% endfor %}
        </marquee>
    </div>
</div>









